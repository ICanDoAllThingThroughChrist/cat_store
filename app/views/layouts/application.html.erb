<!DOCTYPE html>
<html>
      <%= csrf_meta_tags %>
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <head>
    <title>
      <% if content_for?(:title) %>
          <%= yield (:title) %>
      <% else %>
          Cat Store
      <% end %>
      </title>
  </head>
  <body>
  <fieldset>
    <%= render file: "/Users/charlielee/Development/cat_store/app/views/layouts/_header" %>
  <fieldset>
    <%= yield %>
    <% flash.each do |key, message| %>
    <div><%= message %></div>
    <% end %>
  </body>
</html>
