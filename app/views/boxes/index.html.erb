   <h1>Boxes</h1>
        <%= link_to "New Box", new_box_path %>
        <div id="Boxes">
          <% @boxes.each do |box| %>
            <h2>Box Title: <%= link_to box.title, box %></h2>
            <p>Year: <%= box.year %></p>
            <p>Month: <%= box.month %></p>
            <p><% box.items.each do |item| %>
            <p><%= "#{item.title}"%></p>
            <% end %>
            <%#= link_to "Edit Box", edit_box_path(box) %>
          <% end %>
        </div>
