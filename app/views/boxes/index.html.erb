   <h1>Boxes</h1>
        <%= link_to "New Box", new_box_path %>
        <div id="Boxes">
          <% @boxes.each do |box| %>
            <div class="well well-sm">
              <h2>Box Title: <%= link_to box.title, box %></h2>
              <p>Year: <%= box.year %></p>
              <p>Month: <%= box.month %></p>
        </div>
          <% end %>
