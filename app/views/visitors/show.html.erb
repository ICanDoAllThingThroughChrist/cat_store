<div>
  <header>
    <h1><%= @subscription.level %></h1>
  </header>
  <header>
    <h2><%= @visitor.first_name %></h2>
    <table>
    <tr>
    <th>Subscriber:</th>
    <td><%=@visitor.subscriber.email %></td>
    </tr>
    <tr>
    <th>Created:</th>
    <td><%= time_ago_in_words(@visitor.created_at)%>ago</td>
    </tr>
</header>
   <li><%= @visitor.last_name %></li>
   <li><%= @visitor.email %></li>
</div>
<header>
<h2><% @visitor.first_name %></h2>
<ul>
<li><%= link_to "Edit Visitor Subscription"%>, [:edit, @subscription, @visitor]</li>
<li><%= link_to "Delete Visitor from Suscription", [:@subscription, @visitor], method: :delete,
data: {confirm: "Are you sure you want to delete this visitor's subscription?"}, 
class: "delete"%> </li>
</ul>
</header>